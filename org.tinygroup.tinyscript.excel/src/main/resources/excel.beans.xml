<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="readExcelFunction" scope="singleton"
          class="org.tinygroup.tinyscript.excel.function.ReadExcelFunction"> 
    </bean>
    
    <bean id="readCsvFunction" scope="singleton"
          class="org.tinygroup.tinyscript.csv.function.ReadCsvFunction"> 
    </bean>
    
    <bean id="writeExcelFunction" scope="singleton"
          class="org.tinygroup.tinyscript.excel.function.WriteExcelFunction"> 
    </bean>
    
    <bean id="writeCsvFunction" scope="singleton"
          class="org.tinygroup.tinyscript.csv.function.WriteCsvFunction"> 
    </bean>
  
</beans>
