<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="executeSqlFunction" scope="singleton"
          class="org.tinygroup.tinyscript.database.function.ExecuteSqlFunction"> 
    </bean>
    
    <bean id="querySqlFunction" scope="singleton"
          class="org.tinygroup.tinyscript.database.function.QuerySqlFunction"> 
    </bean>
    
    <bean id="jDBCNamedSqlProcessor" scope="singleton"
          class="org.tinygroup.tinyscript.database.sql.JDBCNamedSqlProcessor"> 
    </bean>
    
    
</beans>
