class NewUser {
    
    name,age;
    
    NewUser(name,age){
      this.name = name;
      this.age =age;
    }
    
    NewUser(name){
      this.name = name;
    }
    
    //用户自定义的get/set方法
    getAge(){
      return 12;
    }
}

user = new NewUser("yancheng",35);
user2 = new NewUser("tom");
user3 = new NewUser();  //无参构造函数
println(user);
println(user2);
println(user3);

println(user.getName()); //脚本类支持缺省get/set方法
user.setName("dog");
println(user.getName());
println(user.getAge());  //用户覆盖了get/set方法，以用户为准